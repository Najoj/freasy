<!--<?xml version="1.0" encoding="utf-8"?>-->

<request>

	<match_by>
		<attribute>appName</attribute> 
		<operator>LIKE</operator> 
		<value>a%</value> 
	</match_by>
	
	<match_by>
		<attribute>appName</attribute> 
		<operator>LIKE</operator> 
		<value>%z</value> 
	</match_by>
	
	<match_by>
		<attribute>appId</attribute> 
		<operator>=</operator> 
		<value>4711</value> 
	</match_by>
	
	<order_by>
		<attribute>appName</attribute>
		<direction>DESC</direction>
	</order_by>

	<answer_format>
		<offset>17</offset>
		<number_of_objects>10</number_of_objects>    
	</answer_format>

	<pad_reference_object>
		<appId/>
		<appName/>
	</pad_reference_object>

</request>

