<?xml version="1.0" encoding="utf-8"?>

<request>

	<select>
		<match_by>
			<attribute>appName</attribute> 
			<operator>LIKE</operator> 
			<value>a%</value> 
		</match_by>
		<order_by>
			<attribute>appName</attribute>
			<!--<direction>DESC</direction>-->
		</order_by>
	</select>

	<answer_format>
		<offset>0</offset>
		<number_of_objects>10</number_of_objects>    
	</answer_format>

	<pad_reference_object>
		<appId/>
		<appName/>
	</pad_reference_object>

</request>

